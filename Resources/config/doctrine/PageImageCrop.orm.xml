<?xml version="1.0" encoding="UTF-8"?>

<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">

    <entity name="Aropixel\PageBundle\Entity\PageImageCrop" repository-class="Aropixel\PageBundle\Repository\PageImageCropRepository" table="aropixel_page_image_crop">

        <id name="id" column="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        <many-to-one field="image" target-entity="Aropixel\PageBundle\Entity\PageImage" inversed-by="crops" />

    </entity>

</doctrine-mapping>
